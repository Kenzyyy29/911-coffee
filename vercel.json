{
 "version": 2,
 "blob": true,
 "headers": [
  {
   "source": "/api/blob",
   "headers": [
    {"key": "Access-Control-Allow-Origin", "value": "*"},
    {"key": "Access-Control-Allow-Methods", "value": "GET,POST,OPTIONS"}
   ]
  }
 ],
 "env": {
  "BLOB_READ_WRITE_TOKEN": "@blob-read-write-token"
 }
}
